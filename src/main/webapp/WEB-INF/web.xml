<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Project2</display-name>
  <servlet>
	  <servlet-name>login</servlet-name>
	  <servlet-class>csci310.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
	  <servlet-name>register</servlet-name>
	  <servlet-class>csci310.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
	<servlet-name>addStock</servlet-name>
	<servlet-class>csci310.servlet.AddStockServlet</servlet-class>
  </servlet>
  <servlet>
	<servlet-name>viewStock</servlet-name>
	<servlet-class>csci310.servlet.ViewStockServlet</servlet-class>
  </servlet>
  <servlet>
	<servlet-name>signOut</servlet-name>
	<servlet-class>csci310.servlet.SignOutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>signOut</servlet-name>
  <url-pattern>/api/sio</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/api/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>register</servlet-name>
  <url-pattern>/api/reg</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>addStock</servlet-name>
  	<url-pattern>/api/addstock</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>viewStock</servlet-name>
  	<url-pattern>/api/viewstock</url-pattern>
  </servlet-mapping>
</web-app>